{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "pass",
  "title": "Pass",
  "description": "Interfaces with the standard unix password manager (pass).",
  "icon": "extension_icon.png",
  "author": "snoozedDev",
  "categories": [
    "Security",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Password",
      "subtitle": "Standard Unix Password Manager",
      "description": "Search for a password under the standard unix password manager and copies it. Compatible with OTP.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.8",
    "@raycast/utils": "^1.4.8",
    "@xstate/react": "^3.2.1",
    "xstate": "^4.37.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  },
  "version": "1.0.0"
}
